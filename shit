

    location /info.php {
        #proxy_pass http://apache:8080;
        proxy_pass https://bbacm4cqh1cin1olagcv.containers.yandexcloud.net;
    }




user nginx;
worker_processes auto;
pid /run/nginx.pid;
error_log /var/log/nginx/error.log;
#include /etc/nginx/modules-enabled/*.conf;
# include /etc/nginx/modules/*.conf;
load_module /etc/nginx/modules/ngx_http_image_filter_module.so;


events {
	worker_connections 768;
}

http {

	sendfile on;
	tcp_nopush on;
	types_hash_max_size 2048;

	include /etc/nginx/mime.types;
	default_type application/octet-stream;

	access_log /var/log/nginx/access.log;

	gzip on;

	server {

		root /var/www/html;

		index index.html index.htm index.nginx-debian.html;

		server_name localhost;

		location / {
			try_files $uri $uri/ =404;
		}

		location /cpu {
			root /var/www/html;
			index index.html;
			try_files $uri $uri/ =404;
		}

		location /secondpage {
			root /var/www/html;
			index index.html
			try_files $uri $uri/ =404;
		}

		location /ya {
			proxy_pass https://ya.ru;
			proxy_set_header Host www.ya.ru;
			proxy_set_header X-Real-IP $remote_addr;
			proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
			proxy_set_header X-Forwarded-Proto $scheme;

			proxy_set_header User-Agent $http_user_agent;
		}

		location /php {
			rewrite ^/php(.*)$ /index.php break;
			proxy_pass https://bbaorgvghp0mncj1mugv.containers.yandexcloud.net/;
		}

		location ~* \.jpg$ {
			root /var/www/html/images;
			image_filter rotate 180;
			try_files $uri $uri/ =404;
		}

		location ~* \.png$ {
			root /var/www/html/images;
			try_files $uri $uri/ =404;
		}

		location ~* \.wav {
			root /var/www/html/sounds;
			try_files $uri $uri =404;
		}

		listen 1337;
	}

}
